<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%05r |-%-5level [%thread] %logger - %message%n</pattern>
        </encoder>
    </appender>
    <logger name="com.google.javascript.jscomp" level="off"/>
    <logger name="twibs.web.StaticErrorResponder" level="off"/>
    <logger name="twibs.web.ErrorResponder" level="off"/>
    <root level="WARN">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>
